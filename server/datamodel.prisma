type Session {
    id: ID! @id
    timeStart: DateTime!
    timeEnd: DateTime!
    locations: [Location]!
    mood: Float
    journal: String
  }

  type Map {
    id: ID! @id
    name: String!
    vicinity: String!
    plus_code: PlusCode
    photos: Array
    opening_hours: Boolean
    features: [Feature]
    boundaries: [GeoPoint]
  }

  type Geometry {
    locatioin: Location
    viewport: Viewport
  }
  type Location {
    lat: Float
    Lng: FLoat
  }
  type Viewport {
    northeast: NorthEast
    southwest: SouthWest
  }
  type NorthEast{
    lat: Float
    ling: FLoat
  }
  type SouthWest{
    lat: Float
    Lang: Float
  }
  type PlusCode{
    compound_code:String
    global_code: String
  }
  type Feature {
    id: ID! @id
    title: String!
  }

  type GeoPoint {
    id: ID! @id
    boundary: Json!
  }

  type Goal {
    id : ID! @id
    hours: Int!
    days: [Days]
  }

  type Days {
    id: ID! @id
    title: String!
  }

  type Progress {
    duration: Float!
    completion: Float!
  }