type Session {
    id: ID! @id
    timeStart: DateTime!
    timeEnd: DateTime!
    locations: [Location]!
    mood: Int
    journal: String
  }

  type Location {
    id: ID! @id
    title: String!
    address: String!
    features: [Feature]!
    boundaries: [GeoPoint]!
  }

  type Feature {
    id: ID! @id
    title: String!
  }

  type GeoPoint {
    id: ID! @id
    boundary: Json!
  }

  type Goal {
    id : ID! @id
    hours: Int!
    days: [Days]
  }

  type Days {
    id: ID! @id
    title: String!
  }

  type Progress {
    duration: Float!
    completion: Float!
  }