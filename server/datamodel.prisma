type Session {
    id: ID! @id
    timeStart: String!
    timeEnd: String!
    locations: [Location]!
    mood: Int
    journal: String
  }

  type Location {
    id: ID! @id
    title: String!
    address: String!
    features: [Feature]!
    boundaries: [GeoPoint]!
  }

  type Feature {
    id: ID! @id
    title: String!
  }

  type GeoPoint {
    id: ID! @id
    lat: Float!
    long: Float!
  }